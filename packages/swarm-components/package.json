{
  "name": "@meetup/swarm-components",
  "version": "0.12.1",
  "description": "Web Components for Swarm",
  "author": "hsbacot <hsbacot@gmail.com>",
  "homepage": "https://github.com/meetup/swarm-ui#readme",
  "license": "ISC",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meetup/swarm-ui.git"
  },
  "scripts": {
    "test": "yarn test:integration",
    "test:integration": "NODE_ENV=test jest --config jest.config.int.json",
    "build": "npx babel src --out-dir lib --ignore 'src/**/*.test.js','src/**/*.test.jsx','src/testUtils/**/*'",
    "develop": "npx babel src/ --watch -d lib/"
  },
  "bugs": {
    "url": "https://github.com/meetup/swarm-ui/issues"
  },
  "bundlesize": [
    {
      "path": "lib/*.js",
      "maxSize": "> 10%"
    }
  ],
  "peerDependencies": {
    "react": "^16.6.3"
  },
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-transform-async-to-generator": "7.4.0",
    "@babel/plugin-transform-runtime": "7.4.3",
    "@babel/preset-env": "7.4.3",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "babel-plugin-react-docgen": "2.0.2",
    "flow-bin": "0.96.0",
    "get-port": "^5.0.0",
    "hapi": "18.1.0",
    "inert": "5.1.3",
    "jest": "24.7.1",
    "jest-image-snapshot": "2.8.1",
    "puppeteer": "1.14.0"
  },
  "dependencies": {
    "@meetup/swarm-constants": "^0.3.1",
    "autosize": "^4.0.2",
    "classnames": "2.2.6",
    "swarm-icons": "3.8.354"
  }
}
