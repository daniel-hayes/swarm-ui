{
	"setupFiles": [
		"<rootDir>/src/utils/testSetup.js"
	],
	"moduleFileExtensions": [
		"js",
		"jsx",
		"node",
		"json"
	],
	"testRegex": "(src|util|scripts)/.*\\.test\\.jsx?$",
	"transformIgnorePatterns": [
		"node_modules"
	],
	"snapshotSerializers": [
		"<rootDir>/../../node_modules/enzyme-to-json/serializer"
	]
}
